# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# App Environment
NODE_ENV=development
EXPO_PUBLIC_ENV=development

# API Configuration
EXPO_PUBLIC_API_BASE_URL=http://localhost:3001/api
EXPO_PUBLIC_API_VERSION=v1

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your-supabase-url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# External API Keys
EXPO_PUBLIC_EVENTBRITE_API_KEY=your-eventbrite-api-key
EXPO_PUBLIC_TICKETMASTER_API_KEY=your-ticketmaster-api-key
EXPO_PUBLIC_NYC_OPENDATA_API_KEY=your-nyc-opendata-api-key

# Analytics & Monitoring (Optional)
EXPO_PUBLIC_SENTRY_DSN=your-sentry-dsn
EXPO_PUBLIC_ANALYTICS_KEY=your-analytics-key

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=false
EXPO_PUBLIC_ENABLE_BIOMETRIC_AUTH=false

# Development Settings
EXPO_USE_FAST_RESOLVER=1
EXPO_DEVTOOLS_LISTEN_ADDRESS=0.0.0.0